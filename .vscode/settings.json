{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.trimAutoWhitespace": true,
    "C_Cpp.autocomplete": "Default",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "${default}",
        "${workspaceFolder}/**",
        "${workspaceFolder}/template"
    ],
    "C_Cpp.default.defines": [
        "LOCAL"
    ],
    "clangd.arguments": [
        "--query-driver=/usr/local/bin/g++",
        "--background-index",
        "--header-insertion=iwyu",
        "--all-scopes-completion",
        "--completion-style=detailed",
        "--cross-file-rename",
        "--enable-config",
        "--pretty",
        "--clang-tidy",
        "--pch-storage=disk",
    ],
    "files.associations": {
        "BUILD": "starlark",
        "type_traits": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "regex": "cpp",
        "valarray": "cpp",
        "__hash_table": "cpp",
        "__locale": "cpp",
        "__string": "cpp",
        "algorithm": "cpp",
        "filesystem": "cpp",
        "iosfwd": "cpp",
        "iterator": "cpp",
        "__split_buffer": "cpp",
        "bitset": "cpp",
        "map": "cpp",
        "set": "cpp",
        "__bit_reference": "cpp",
        "__tree": "cpp",
        "chrono": "cpp",
        "compare": "cpp",
        "ratio": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "limits": "cpp",
        "cassert": "cpp",
        "cstdalign": "cpp",
        "ios": "cpp",
        "tuple": "cpp"
    },
    "cSpell.words": [
        "arithmatex",
        "autohide",
        "betterem",
        "chmax",
        "chmin",
        "escapeall",
        "fontawesome",
        "hardbreak",
        "inlinehilite",
        "leetcode",
        "linenums",
        "magiclink",
        "mkdocs",
        "nbsp",
        "noopener",
        "noreferrer",
        "noreply",
        "pbds",
        "peaceiris",
        "popcountll",
        "progressbar",
        "pymdownx",
        "Roboto",
        "saneheaders",
        "smartsymbols",
        "striphtml",
        "superfences",
        "tasklist"
    ],
    "cSpell.enabled": false,
    "yaml.customTags": [
        "!ENV scalar",
        "!ENV sequence",
        "tag:yaml.org,2002:python/name:materialx.emoji.to_svg",
        "tag:yaml.org,2002:python/name:materialx.emoji.twemoji",
        "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format",
        "tag:yaml.org,2002:python/name:pymdownx.slugs.uslugify",
        "tag:yaml.org,2002:python/name:pymdownx.arithmatex.inline_mathjax_format",
        "tag:yaml.org,2002:python/name:pymdownx.arithmatex.fence_mathjax_format"
    ]
}
